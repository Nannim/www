  var ad_label = document.createElement('div');
  ad_label.style.textAlign = 'center';
  ad_label = tmntag_adlabel.setStyle(divid,ad_label);
  var insertedNode = purch_div.insertBefore(ad_label, purch_div.childNodes[0]);
}

