    /*ad_label.style.setProperty('cursor', 'pointer', 'important');
    ad_label.onclick = function(){
      tmntag.openReportingPopup(divid);
    }*/
  }
  var styleParams = (adParams && adParams['ad_label_style']);
  if (adParams && styleParams) {
    styleParams = styleParams.trim().split(';');
    for (var i = 0; i < styleParams.length; i++) {
      if(styleParams[i] === ''){continue;}
      style = styleParams[i].trim().split(':');
      var importance = null;
      if (!style[1]){continue;}
      if (style[1].indexOf('!important') > 0) {
        style[1] = style[1].replace('!important', '');
        importance = 'important';
      }
      ad_label.style.setProperty(style[0].trim(),style[1].trim(),importance);
    }
  }
  return ad_label;
}
